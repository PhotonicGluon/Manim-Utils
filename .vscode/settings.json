{
    "files.exclude": {
        "**/.ipynb_checkpoints": true,
        "**/__pycache__": true,
        "venv": true
    },
    "cSpell.words": [
        "centering",
        "coor",
        "fruchterman",
        "gens",
        "hasse",
        "linalg",
        "manim",
        "mobject",
        "mobjects",
        "ndarray",
        "networkx",
        "reingold"
    ],
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "black-formatter.args": [
        "--line-length=120"
    ],
    "autoDocstring.docstringFormat": "google-notypes",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
}